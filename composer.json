{
  "name": "focusconcursos/sambatech-laravel",
  "description": "A wrapper of the Sambatech API, designed to Laravel 5+.",
  "version": "1.0",
  "license": "MIT",
  "keywords": [
    "laravel",
    "sambatech",
    "video"
  ],
  "require": {
    "php": ">=7.1.0",
    "guzzlehttp/guzzle": "^6.3",
    "illuminate/support": "5.7.*",
    "graham-campbell/manager": "^4.0",
    "ext-json": "*"
  },
  "autoload": {
    "psr-4": {
      "FocusConcursos\\SambatechLaravel\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "FocusConcursos\\Tests\\SambatechLaravel\\": "tests/"
    }
  },
  "config": {
    "preferred-install": "dist"
  },
  "extra": {
    "laravel": {
      "providers": [
        "FocusConcursos\\SambatechLaravel\\SambatechServiceProvider"
      ],
      "aliases": {
        "Sambatech": "FocusConcursos\\SambatechLaravel\\Facades\\Sambatech"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
